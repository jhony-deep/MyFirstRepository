<apex:page controller="Practice" >
    <apex:form >
   <apex:pageBlock >
        <apex:pageBlockSection columns="1">
       <apex:inputField value="{!acc.name}"/>
         <apex:inputField value="{!acc.Phone}"/>
              <apex:inputField value="{!acc.Industry}"/>
              <apex:inputField value="{!acc.Rating}"/>
              <apex:inputField value="{!acc.AnnualRevenue}"/>
            jsonstring<apex:inputTextarea value="{!jsonstring}" cols="50" rows="10"/>
              xmlstring<apex:inputTextarea value="{!xmlstring}" cols="50" rows="10"/>
       </apex:pageBlockSection>
        </apex:pageBlock>
    <apex:commandButton value="create" action="{!Practice}"/>
        </apex:form>
</apex:page>