<apex:page standardController="Account">
    <apex:form >
    <apex:sectionHeader title="Account Details" subtitle="Details" help="PlzhLp"/>
        <apex:pageBlock title="Plz Fill Your Form" tabStyle="opportunity">
            <apex:pageBlockButtons location="top">
            <apex:commandButton value="Save" action="{!save}"/>
            </apex:pageBlockButtons>
        <apex:pageBlockSection title="Fill Details" collapsible="false">
            <apex:pageBlockSectionItem >
                <apex:outputText >Enter Your Name</apex:outputText>
            <apex:inputField value="{!Account.Name}"/>
            </apex:pageBlockSectionItem>
               <apex:pageBlockSectionItem >
                <apex:outputText >Enter Your Industry</apex:outputText>
            <apex:inputField value="{!Account.Industry}"/>
            </apex:pageBlockSectionItem>
                 <apex:pageBlockSectionItem >
                <apex:outputText >Enter Your Phone</apex:outputText>
            <apex:inputField value="{!Account.Phone}"/>
            </apex:pageBlockSectionItem>
              <apex:pageBlockSectionItem >
                <apex:outputText >Enter Your rating</apex:outputText>
            <apex:inputField value="{!Account.rating}"/>
            </apex:pageBlockSectionItem>
            <apex:inputText value="{!account.id}"/>
            </apex:pageBlockSection>
        </apex:pageBlock>
    </apex:form>
</apex:page>